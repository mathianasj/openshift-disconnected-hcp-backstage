kind: ImageSetConfiguration
apiVersion: mirror.openshift.io/v1alpha2
storageConfig:
  registry:
    imageURL: "{{ quayHostname }}:8443/mirror/oc-mirror-metadata"
mirror:
  additionalImages:
    - name: quay.io/tigera/operator:v1.32.7
    - name: docker.io/calico/ctl:v3.27.3
    - name: docker.io/calico/csi:v3.27.3
    - name: docker.io/calico/node-driver-registrar:v3.27.3
